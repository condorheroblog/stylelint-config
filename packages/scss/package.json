{
  "name": "@condorhero/stylelint-config-scss",
  "version": "1.0.0-beta.1",
  "packageManager": "pnpm@7.1.6",
  "description": "My Stylelint SCSS config presets",
  "keywords": [
    "eslint",
    "Stylelint-config-scss",
    "Stylelint-config-preset"
  ],
  "license": "MIT",
  "author": "CondorHero",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/condorheroblog/stylelint-config.git",
    "directory": "packages/scss"
  },
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "stylelint:fix": "stylelint . --fix --cache --cache-location node_modules/.cache/stylelint/ --config=index.js",
    "stylelint": "stylelint . --config=index.js"
  },
  "peerDependencies": {
    "stylelint": ">=14.9.0"
  },
  "bugs": {
    "url": "https://github.com/condorheroblog/stylelint-config/issues"
  },
  "homepage": "https://github.com/condorheroblog/stylelint-config#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@condorhero/stylelint-config-css": "workspace:^",
    "stylelint-config-standard-scss": "^4.0.0"
  },
  "devDependencies": {
    "stylelint": "^14.9.0"
  }
}
